{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Blazor WebAssembly in Firefox",
            "type": "blazorwasm",
            "request": "launch",
            "hosted": true,
            "browser": "edge",
            "url": "https://localhost:5299", // Replace with your hosted app's URL if different
            "trace": "verbose",
            "webRoot": "${workspaceFolder}/wwwroot"
        }
    ]
}
